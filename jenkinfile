pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Build'
                sleep 10
            }
        }
        stage('Test') {
            steps {
                echo 'Test step'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Step'
                sleep 10
            }
        }
        }
    }
}
